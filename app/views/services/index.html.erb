<div class="container">
  <p id="notice"><%= notice %></p>
  
  <h1>Atenciones</h1>
  
  <table>
    <thead>
      <tr>
        <th>Nombre</th>
        <th>Monto unitario</th>
        <th>Nombre Cliente</th>
        <th>Nombre Profesional</th>
        <th>Numero de atenciones</th>
        <th>Monto total</th>
        <th>Calificaci√≥n</th>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @services.each do |service| %>
        <tr>
          <td><%= service.name %></td>
          <td><%= service.amount %></td>
          <td><%= service.client.name %></td>
          <td><%= service.collaborator.name %></td>
          <td><%= service.session %></td>
          <td><%= service.bill %></td>
          <td><%= service.score %></td>
          <td><%= link_to 'Edit', edit_service_path(service) %></td>
          <td><%= link_to 'Destroy', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>
  
  <%= link_to 'Back', admins_path %>

</div>
